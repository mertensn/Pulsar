<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  
  <appSettings>
    <!--TODO Nicolas: Change direct address from svc client-->
    <add key="ServiceClient:Services.HouseManagerService" value="Direct(Address=http://telephonydirectory.test.local/Subscriptiondata.svc?wsdl)" />
